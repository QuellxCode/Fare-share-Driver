<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#F2F2F2"
    android:fitsSystemWindows="true">
    <include
        android:id="@+id/tb"
        layout="@layout/appbar" />
    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fitsSystemWindows="true"
        android:orientation="vertical">
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="155dp"
            android:layout_margin="5dp"
            android:fitsSystemWindows="true"
            app:cardCornerRadius="8dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:weightSum="1">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:background="#99CCFF"
                    android:gravity="center"
                    android:text="Work History"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    android:textSize="15dp"
                    android:textColor="@color/colorBlack"
                    android:textStyle="bold" />
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/linearLayout2">

                    <TextView
                        android:id="@+id/pid"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_above="@+id/pname1"
                        android:gravity="center"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="25dp"
                        android:text="Patner ID"
                        android:textColor="@color/colorBlack" />
                    <TextView
                        android:id="@+id/pname"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorBlack"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginStart="8dp"
                        android:text="Month" />
                    <TextView
                        android:id="@+id/oc"
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorBlack"
                        android:layout_marginLeft="8dp"
                        android:gravity="center"
                        android:layout_marginTop="15dp"
                        android:layout_marginStart="8dp"
                        android:text="Order Compleate" />
                </LinearLayout>

                <android.support.v7.widget.LinearLayoutCompat
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_toEndOf="@+id/linearLayout2"
                    android:layout_toRightOf="@+id/linearLayout2"
                    android:orientation="vertical"
                    android:id="@+id/linearLayoutCompat">

                    <TextView
                        android:id="@+id/pid1"
                        android:layout_width="50dp"
                        android:layout_height="20dp"
                        android:layout_above="@+id/pname1"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="25dp"
                        android:text="123"
                        />

                    <TextView
                        android:id="@+id/pname1"
                        android:layout_width="50dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="15dp"
                        android:text="April"
                        />
                    <TextView
                        android:id="@+id/oc1"
                        android:layout_width="50dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="25dp"
                        android:text="10"
                        />

                </android.support.v7.widget.LinearLayoutCompat>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/linearLayoutCompat"
                    android:layout_alignParentTop="true"
                    android:layout_alignStart="@+id/linearLayoutCompat"
                    android:layout_marginLeft="90dp"
                    android:layout_marginStart="90dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/ordrej"
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_above="@+id/pname1"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="25dp"
                        android:text="Order Rejected"
                        android:gravity="center"
                        android:textColor="@color/colorBlack" />

                    <TextView
                        android:id="@+id/acp"
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="3dp"
                        android:text="Accept.. Rate"
                        android:textColor="@color/colorBlack" />

                    <TextView
                        android:id="@+id/total"
                        android:layout_width="70dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center"
                        android:text="Toatal Earning"
                        android:textColor="@color/colorBlack" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="260dp">
                    <TextView
                        android:id="@+id/or1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_above="@+id/pname1"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="35dp"
                        android:text="123"
                        />
                    <TextView
                        android:id="@+id/acp1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="12dp"
                        android:text="4.4"
                        />
                    <TextView
                        android:id="@+id/total1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="25dp"
                        android:text="3224"
                        />



                </LinearLayout>



                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_margin="5dp"
                    android:background="#2196F3"
                    android:layout_alignParentStart="true"
                    android:id="@+id/textView2">

                </TextView>

            </RelativeLayout>

        </android.support.v7.widget.CardView>
    </android.support.v7.widget.LinearLayoutCompat>





</LinearLayout>